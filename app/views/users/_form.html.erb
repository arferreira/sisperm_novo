<%= form_for @user do |f| %>

  <div class="form-group">
    <%= f.label :login, :class => 'control-label' %>
    <%= f.text_field :login, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :remember_created_at, :class => 'control-label' %>
    <%= f.datetime_select :remember_created_at, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :sign_in_count, :class => 'control-label' %>
    <%= f.number_field :sign_in_count, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :current_sign_in_at, :class => 'control-label' %>
    <%= f.datetime_select :current_sign_in_at, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :last_sign_in_at, :class => 'control-label' %>
    <%= f.datetime_select :last_sign_in_at, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :current_sign_in_ip, :class => 'control-label' %>
    <%= f.text_field :current_sign_in_ip, :class => 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :last_sign_in_ip, :class => 'control-label' %>
    <%= f.text_field :last_sign_in_ip, :class => 'form-control' %>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                users_path, :class => 'btn' %>
  </div>
<% end %>
